library ieee;
use ieee.std_logic_1164.all;

entity bitextender is
    
    port (
        A: in std_logic_vector(8 downto 0);
        O: out std_logic_vector(15 downto 0)
    ) ;
end bitextender;
architecture behavior of bitextender is

begin
alu : process( A )
variable temp : std_logic_vector(8 downto 0);
begin

if temp(8) ='0' then
O <= "0000000"&A;
elsif temp(8) ='1' then 
O <=  "1111111"&A;
end if;

end process ; -- alu
end behavior;